{
    "description": "Swap bracket keys: [ and ] become default, + and Â¨ require Option modifier",
    "manipulators": [
        {
            "from": {
                "key_code": "open_bracket",
                "modifiers": { "optional": ["caps_lock", "control", "command"] }
            },
            "to": [
                {
                    "key_code": "open_bracket",
                    "modifiers": ["left_option"]
                }
            ],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "open_bracket",
                "modifiers": {
                    "mandatory": ["left_option"],
                    "optional": ["caps_lock", "control", "command"]
                }
            },
            "to": [{ "key_code": "open_bracket" }],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "open_bracket",
                "modifiers": {
                    "mandatory": ["right_option"],
                    "optional": ["caps_lock", "shift", "control", "command"]
                }
            },
            "to": [{ "key_code": "open_bracket" }],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "close_bracket",
                "modifiers": { "optional": ["caps_lock", "control", "command"] }
            },
            "to": [
                {
                    "key_code": "close_bracket",
                    "modifiers": ["left_option"]
                }
            ],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "close_bracket",
                "modifiers": {
                    "mandatory": ["left_option"],
                    "optional": ["caps_lock", "control", "command"]
                }
            },
            "to": [{ "key_code": "close_bracket" }],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "close_bracket",
                "modifiers": {
                    "mandatory": ["right_option"],
                    "optional": ["caps_lock", "control", "command"]
                }
            },
            "to": [{ "key_code": "close_bracket" }],
            "type": "basic"
        }
    ]
}
